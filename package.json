    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "prettier": "^3.0.0",
    "typescript": "^5.8.3"
  }
}
